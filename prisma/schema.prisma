generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_PRISMA_URL")
}

model Info {
  id          Int     @id @default(autoincrement())
  temperature Decimal @db.Decimal(5, 2)
  lowWaterA   Boolean
  lowWaterB   Boolean
  lowWaterC   Boolean
  lowWaterD   Boolean
}


model Settings {
  id        Int @id @default(autoincrement())
  flowRateA Int
  flowRateB Int
  flowRateC Int
  flowRateD Int
}
